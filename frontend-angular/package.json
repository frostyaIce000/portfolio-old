{
  "name": "angular-starter",
  "version": "20.2.3",
  "scripts": {
    "ng": "ng",
    "dev": "ng serve --port 4200",
    "start": "ng serve --port 4200",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:headless": "ng test --watch=false --browsers=ChromeHeadless",
    "coverage": "ng test --no-watch --code-coverage",
    "lint": "ng lint",
    "serve": "node dist/angular-starter/server/server.mjs",
    "generate-project-structure": "node tools/scripts/generate-project-structure.js",
    "docker:build": "docker build -t frontend-angular .",
    "docker:run": "docker run -p 4000:4000 frontend-angular"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "20.2.4",
    "@angular/compiler": "20.2.4",
    "@angular/core": "20.2.4",
    "@angular/forms": "20.2.4",
    "@angular/platform-browser": "20.2.4",
    "@angular/platform-server": "20.2.4",
    "@angular/router": "20.2.4",
    "@angular/service-worker": "20.2.4",
    "@angular/ssr": "20.2.2",
    "@fortawesome/fontawesome-free": "7.0.1",
    "bootstrap": "5.3.8",
    "express": "5.1.0",
    "rxjs": "7.8.2",
    "tslib": "2.8.1",
    "uuid": "13.0.0",
    "zone.js": "0.15.1"
  },
  "devDependencies": {
    "@angular/build": "20.2.2",
    "@angular/cli": "20.2.2",
    "@angular/compiler-cli": "20.2.4",
    "@types/express": "5.0.3",
    "@types/jasmine": "5.1.9",
    "@types/node": "24.3.1",
    "angular-eslint": "20.2.0",
    "eslint": "9.35.0",
    "jasmine-core": "5.10.0",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "typescript": "5.8.3",
    "typescript-eslint": "8.43.0"
  }
}
